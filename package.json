{
  "name": "@developerportalsg/docsify-sidebar-collapse",
  "version": "2.0.23",
  "type": "module",
  "description": "A docsify plugin for a collapsible sidebar!",
  "author": "Luth Andyka <luthandyka.business@gmail.com>",
  "keywords": [
    "docsify",
    "plugin",
    "sidebar",
    "collapse"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sg-developer-portal/docsify-sidebar-collapse.git"
  },
  "files": [
    "dist/index.js",
    "dist/index.css"
